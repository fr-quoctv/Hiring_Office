<% if @addresses.any? %>
  <% @addresses.each do |address| %>
    <% unless address.venue.try(:block) %>
      <% if address.venue.try(:status) %>
        <%= render address.spaces %>
      <% end %>
    <% end %>
  <% end %>
<% else %>
  <div class="col-sm-8 col-sm-offset-2 text-center">
    <h3><%= t "errors.spaces.no_result" %></h3>
  </div>
<% end %>
