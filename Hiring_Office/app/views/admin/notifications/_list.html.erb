<% @notifications_admin.each do |notification| %>
  <li class="<%= status_unread notification %> mark-read-admin" data-id="<%= notification.id %>">
  <i class="fa fa-flag-checkered" aria-hidden="true"></i>
    <span>
      <%= link_to notification.load_message,
        admin_venues_path,
        class: "style-notification" %>
    </span>
  </li>
  <hr class="hr-notification"></hr>
<% end %>
